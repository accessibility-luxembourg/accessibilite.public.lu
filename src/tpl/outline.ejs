<ul>
<% config[lang].mainMenu.concat(config[lang].footer).forEach(element => { if (element.name !== 'plan-site' && element.children === undefined) { %>
    <li><a href="<%= prefix %>/<%= lang %>/<%= element.name %>.html"><%= element.title %></a></li>
<% }}); %>
</ul>
<% config[lang].mainMenu.concat(config[lang].footer).forEach(element => { if (element.name !== 'plan-site' && element.children !== undefined) { %>
    <h3><%= element.title %></h3>
    <ul>
        <% element.children.forEach(child => { %>
            <% if (child.url !== undefined) { %>
                <li><a href="<%= prefix %>/<%= lang %>/<%= child.url %>"><%= child.menu %></a></li> 
            <% } else { %>
                <li><a href="<%= prefix %>/<%= lang %>/<%= child.name %>.html"><%= child.menu %></a></li> 
            <% } %>
        <% }) %>
    </ul>
<% }}); %>
