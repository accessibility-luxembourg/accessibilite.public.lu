{
  "name": "rgaa4lu",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "bash ./build.sh",
    "watch:dev": "npm-watch",
    "watch": "concurrently -k 'npm:serve' 'npm:watch:dev'",
    "serve": "http-server ./dist",
    "checklinks": "npx check-html-links dist/ --validate-externals --absolute-base-url https://accessibilite.public.lu",
    "checkaccess": "node ./test/accessTests.js",
    "test:links": "concurrently -k -s first 'npm:serve' 'npm:checklinks'",
    "test:access": "concurrently -k -s first 'npm:serve' 'npm:checkaccess'",
    "test": "npm run test:links && npm run test:access",
    "postinstall": "patch-package"
  },
  "watch": {
    "build": {
      "patterns": [
        "src/tpl",
        "src/css",
        "src/js",
        "content"
      ],
      "extensions": [
        "js",
        "ejs",
        "md",
        "scss",
        "json"
      ]
    }
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "engines.browsers": [
    "defaults"
  ],
  "author": "Information and press service",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.11.2",
    "@pidila/scampi": "^1.0.0",
    "core-js": "^3.6.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.2",
    "en301549-links": "^1.0.7",
    "form-data-entries": "^1.0.4",
    "highcharts": "^10.3.3",
    "object.fromentries": "^2.0.3",
    "papaparse": "^5.3.2",
    "regenerator-runtime": "^0.13.7",
    "sharp": "^0.32.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "axe-webdriverjs": "^2.3.0",
    "babel-types": "^6.26.0",
    "cheerio": "^1.0.0-rc.9",
    "concurrently": "^5.3.0",
    "ejs": "^3.1.2",
    "http-server": "^0.12.3",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-replace-link": "^1.0.1",
    "npm-watch": "^0.6.0",
    "parcel-bundler": "^1.12.4",
    "patch-package": "^6.5.1",
    "sass": "^1.26.10",
    "selenium-webdriver": "^4.0.0-alpha.7"
  }
}
